# Telegram Bot Token (получить у @BotFather)
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Google Service Account (из скачанного JSON файла)
GOOGLE_SERVICE_ACCOUNT_EMAIL=your-service-account@your-project.iam.gserviceaccount.com
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n"

# Google Spreadsheet ID (из URL таблицы: https://docs.google.com/spreadsheets/d/SPREADSHEET_ID/edit)
GOOGLE_SPREADSHEET_ID=your_spreadsheet_id_here

# Google Drive Folder ID (из URL папки: https://drive.google.com/drive/folders/FOLDER_ID)
GOOGLE_DRIVE_FOLDER_ID=your_folder_id_here

